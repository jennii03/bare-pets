    "0123b705-1144-4761-991b-05df5b98015f": {
      "type": "comp-values",
      "blocks": {
        "5baf2506-397c-4295-b06d-b4e2b0113314": {
          "type": "values",
          "settings": {
            "photo": "shopify:\/\/shop_images\/1.png",
            "subtitle": "WE BELIEVE IN",
            "title": "transparency",
            "description": "We aspire to elevate the level of transparency in the industry. From our ingredients and recipes to how different things affect a pet’s health. We aim to bring it all out in the open."
          }
        },
        "a4543436-8df9-409f-b6e6-9c48383fa214": {
          "type": "values",
          "settings": {
            "photo": "shopify:\/\/shop_images\/2.png",
            "subtitle": "WE BELIEVE IN",
            "title": "nutrition",
            "description": "We believe pet food can be both tasty and nutritious. It plays a significant role in a healthy, happy and balanced lifestyle and should make a positive contribution to the health and development of your pet."
          }
        },
        "d38d9a39-530c-4783-86c2-9fcc130db5cc": {
          "type": "values",
          "settings": {
            "photo": "shopify:\/\/shop_images\/3.png",
            "subtitle": "WE BELIEVE IN",
            "title": "togetherness",
            "description": "We believe in bringing people together by creating a community of pet parents, caring for them and their pets, and making a difference to the ones who aren’t as fortunate."
          }
        },
        "8043296e-b52f-4aa8-a328-86da8779e22e": {
          "type": "values",
          "settings": {
            "photo": "shopify:\/\/shop_images\/4.png",
            "subtitle": "WE BELIEVE IN",
            "title": "simplicity",
            "description": "We believe in the power of simplicity. Every experience from the decision on what to purchase to unboxing should feel effortless and convenient for every full time pet parent."
          }
        }
      },
